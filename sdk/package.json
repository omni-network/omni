{
  "name": "@omni-network/solvernet",
  "description": "Typescript interface for Omni Solvernet",
  "version": "0.0.0-alpha.1",
  "type": "module",
  "license": "GPL-3.0-only",
  "packageManager": "pnpm@9.1.0",
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run clean && tsc -p tsconfig.build.json",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "precommit": "biome check --write"
  },
  "files": [
    "dist/**",
    "!dist/**/*.tsbuildinfo",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "!src/**/*.test-d.ts"
  ],
  "engines": {
    "node": "22.x"
  },
  "peerDependencies": {
    "react": ">=18",
    "typescript": ">=5.4.0",
    "viem": ">=2.22.10",
    "wagmi": ">=2.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/react": "^19.0.8",
    "typescript": "5.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omni-network/omni.git",
    "directory": "sdk"
  },
  "keywords": ["typescript", "web3", "ethereum", "omni"],
  "author": "Omni Network",
  "homepage": "https://omni.network/"
}
