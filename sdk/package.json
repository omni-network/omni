{
  "name": "omni-network",
  "description": "Typescript interface for Omni Solvernet",
  "version": "0.0.0-alpha.2",
  "type": "module",
  "license": "MIT",
  "packageManager": "pnpm@9.1.0",
  "sideEffects": false,
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "pnpm clean && pnpm build:esm && pnpm build:types",
    "build:esm": "tsc -p tsconfig.build.json",
    "build:types": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "check": "biome check --write"
  },
  "files": [
    "dist/**",
    "src/**",
    "!dist/**/*.tsbuildinfo",
    "!src/**/*.test.ts",
    "!src/**/*.test-d.ts"
  ],
  "engines": {
    "node": ">=22.x"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.64.2",
    "react": ">=18",
    "viem": ">=2.22.10",
    "wagmi": ">=2.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/react": "^19.0.8",
    "typescript": "5.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omni-network/omni.git",
    "directory": "sdk"
  },
  "keywords": ["typescript", "web3", "ethereum", "omni"],
  "author": "Omni Network",
  "homepage": "https://omni.network/"
}
