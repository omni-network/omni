{
  "name": "omni-sdk",
  "version": "0.0.1",
  "type": "module",
  "license": "GPL-3.0-only",
  "packageManager": "pnpm@9.1.0",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc -p tsconfig.build.json",
    "build:clean": "pnpm clean && pnpm build",
