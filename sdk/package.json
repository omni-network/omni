    "precommit:dry": "biome check --write --dry"
  },
  "files": ["src/*", "dist/*"],
  "engines": {
    "node": "22.x"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "typescript": "5.7.2"
  }
}
